  "bugs": {
    "url": "https://github.com/mcollina/hyperid/issues"
  },
  "homepage": "https://github.com/mcollina/hyperid#readme",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "bloomfilter": "0.0.16",
    "hashids": "^1.1.4",
    "nid": "^0.3.2",
    "pre-commit": "^1.2.2",
    "shortid": "^2.2.8",
    "standard": "^11.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0",
    "typescript": "^2.8.3",
    "zuul": "^3.11.1"
